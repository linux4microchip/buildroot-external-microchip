+bool HSS_LogoInit(void)
+{
+    mHSS_PUTS("\n");
+    int i;
+
+    // decode and output our RLE Logo
+    for (i = 0; i < ARRAY_SIZE(rleLogoElements); i++) {
+        uint8_t j;
+
+        for (j = 0u; j < rleLogoElements[i].count; j++) {
+            mHSS_PUTS(tokenStringTable[rleLogoElements[i].tokenIndex]);
+        }
+    }
