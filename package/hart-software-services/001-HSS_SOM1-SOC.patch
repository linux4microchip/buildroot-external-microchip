+    PROVIDE(__u54_3_itim_end = ORIGIN(u54_3_itim) + LENGTH(u54_3_itim));
+    PROVIDE(__u54_4_itim_start = ORIGIN(u54_4_itim));
+    PROVIDE(__u54_4_itim_end = ORIGIN(u54_4_itim) + LENGTH(u54_4_itim));
+    . = __l2_start;
+    PROVIDE(_hss_start = .);
+    PROVIDE(__l2_scratchpad_vma_start = .);
+    .text : ALIGN(0x10)
+    {
