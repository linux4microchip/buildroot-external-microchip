+        PROVIDE_HIDDEN (__fini_array_start = .);
+        KEEP (*(.fini_array))
+        KEEP (*(SORT(.fini_array.*)))
+        PROVIDE_HIDDEN (__fini_array_end = .);
+        . = ALIGN(0x10);
+    } >l2zerodevice
+    .gnu_build_id : ALIGN(8) {
+        PROVIDE(gnu_build_id = .);
+        *(.note.gnu.build-id)
